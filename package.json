{
  "name": "code-review-action",
  "version": "1.0.0",
  "description": "Multi-provider AI code review GitHub Action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build src/index.ts -o dist",
    "build:only": "ncc build src/index.ts -o dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:report": "jest --coverage --verbose",
    "test:performance": "jest --verbose --detectOpenHandles",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "coverage:report": "node scripts/generate-coverage-report.js",
    "coverage:badge": "node scripts/generate-coverage-badge.js",
    "test:trends": "node scripts/analyze-test-trends.js",
    "test:readme": "node scripts/generate-test-readme.js",
    "package": "npm run build"
  },
  "keywords": [
    "github-action",
    "code-review",
    "ai",
    "openai",
    "claude",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-ai/generativelanguage": "^2.4.0",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^20.0.1",
    "google-auth-library": "^9.4.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
